<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login_validation</servlet-name>
        <servlet-class>Login_validation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reg_Validation</servlet-name>
        <servlet-class>Reg_Validation</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Add_event_val</servlet-name>
        <servlet-class>Add_event_val</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reg_event</servlet-name>
        <servlet-class>Reg_event</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete_events</servlet-name>
        <servlet-class>Delete_events</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_event_val</servlet-name>
        <servlet-class>Update_event_val</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_password</servlet-name>
        <servlet-class>Update_password</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Update_pro</servlet-name>
        <servlet-class>Update_pro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login_validation</servlet-name>
        <url-pattern>/Login_validation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reg_Validation</servlet-name>
        <url-pattern>/Reg_Validation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Add_event_val</servlet-name>
        <url-pattern>/Add_event_val</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reg_event</servlet-name>
        <url-pattern>/Reg_event</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete_events</servlet-name>
        <url-pattern>/Delete_events</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_event_val</servlet-name>
        <url-pattern>/Update_event_val</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_password</servlet-name>
        <url-pattern>/Update_password</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Update_pro</servlet-name>
        <url-pattern>/Update_pro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
